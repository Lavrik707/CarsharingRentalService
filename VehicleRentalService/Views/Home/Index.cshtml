@model IEnumerable<VehicleRentalService.Models.Car>

@foreach (var p in Model ?? Enumerable.Empty<VehicleRentalService.Models.Car>())
{
    <div>
        <h3>@p.Name</h3>
        <p>@p.Description</p>
        <h4>
            @p.PricePerMinute.ToString("c", new System.Globalization.CultureInfo("de-DE")) /min
        </h4>
    </div>
}
